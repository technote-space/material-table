{
  "name": "@technote-space/material-table",
  "version": "2.3.0",
  "description": "Datatable for React based on https://material-ui.com/api/table/ with additional features",
  "keywords": [
    "react",
    "material-ui",
    "material",
    "datatable",
    "table"
  ],
  "homepage": "https://github.com/technote-space/material-table",
  "bugs": {
    "url": "https://github.com/technote-space/material-table/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/technote-space/material-table.git"
  },
  "license": "MIT",
  "author": "Mehmet Baran, material-table-core contributors",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "lint": "eslint 'src/**/*.js' --cache",
