    "@material-ui/core": "^4.11.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-polyfill": "^6.26.0",
    "eslint": "^7.17.0",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.22.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "webpack": "^5.12.2"
  },
  "peerDependencies": {
    "@date-io/core": "^2.10.6",
    "@material-ui/core": "^4.11.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "publishConfig": {
    "access": "public"
